<Page x:Class="Rock.Apps.CheckScannerUtility.MicrImageHostPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:ax2="clr-namespace:AxMTMicrImage;assembly=AxMTMicrImage"
      mc:Ignorable="d"
      d:DesignHeight="300"
      d:DesignWidth="300"
      Title="MicrImageHostPage">

    <Grid>
        <WindowsFormsHost x:Name="micrImageHost"
                          Visibility="Collapsed">
            <ax2:AxMicrImage x:Name="micrImage" />
        </WindowsFormsHost>
    </Grid>
</Page>
